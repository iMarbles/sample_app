<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
       <%= @user.name %>, <%= @user.email %>
      </h1>
    </section>
          
    <br/>
    Created at: <%= @user.created_at %>
    <br/>
    Updated at: <%= @user.updated_at %>
    <br/>
    Time Now: <%= Time.now %>
  </aside>
</div>